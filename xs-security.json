{
  "xsappname": "vcpappvcplocproduniqid",
  "tenant-mode": "dedicated",
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "uaa.user",
      "description": "UAA"
    },
    {
      "name": "$XSAPPNAME.User",
      "description": "UAA Display"
    },
    {
      "name": "$XSAPPNAME.read",
      "description": "Read authorization information"
    },
    {
      "name": "$XSAPPNAME.admin",
      "description": "Read authorization information"
    }
  ],
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA",
      "scope-references": [
        "uaa.user"
      ]
    },
    {
      "name": "VCPUserRole",
      "description": "UAA VCPUserRole",
      "scope-references": [
        "$XSAPPNAME.User"
      ]
    },
    {
      "name": "VCPSupportRole",
      "description": "UAA VCPSupportRole",
      "scope-references": [
        "$XSAPPNAME.read"
      ]
    },
    {
      "name": "VCPSupportAdminRole",
      "description": "UAA VCPSupportAdminRole",
      "scope-references": [
        "$XSAPPNAME.admin"
      ]
    },
    {
      "name": "VCPConfigurationRole",
      "description": "UAA VCPConfigurationRole",
      "scope-references": [
        "$XSAPPNAME.admin"
      ]
    },
    {
      "name": "VCPSupport",
      "description": "UAA VCPSupport",
      "scope-references": [
        "$XSAPPNAME.admin"
      ]
    },
    {
      "name": "VCPAdmin",
      "description": "UAA VCPAdmin",
      "scope-references": [
        "$XSAPPNAME.admin"
      ]
    },
    {
      "name": "VCPConfiguration",
      "description": "UAA VCPConfiguration",
      "scope-references": [
        "$XSAPPNAME.admin"
      ]
    }
    
  ],
  "oauth2-configuration": {
    "token-validity": 3600,
    "redirect-uris": [
      "https://*.hana.ondemand.com/**",
      "https://*.applicationstudio.cloud.sap/**"
    ]
  },
  "attributes": [],
  "authorities-inheritance": false
}
